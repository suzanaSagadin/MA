      <dimensions type="negative"><height unit="mm">90</height><width unit="mm">130</width></dimensions></ab>
      <ab type="lido:subject">
        <date when="1930-03-17"></date>
      <country type="contemporary">Serbia</country><country type="historical">Yugoslavia</country><placeName><ref target="info:fedora/context:siba.gn.650f150e" type="context">Belgrade</ref><note>Narodno Pozorište</note></placeName></ab>
