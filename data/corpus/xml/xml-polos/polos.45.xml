ido:inscriptionDescription></lido:inscriptions><lido:inscriptions lido:type="postmark"><lido:inscriptionDescription><lido:descriptiveNoteID lido:type="ID" lido:source="http://id.loc.gov/vocabulary/iso639-2">ger</lido:descriptiveNoteID><lido:descriptiveNoteValue lido:label="info:fedora/context:polos.postmark.ger">German</lido:descriptiveNoteValue></lido:inscriptionDescription></lido:inscriptions></lido:inscriptionsWrap><lido:repositoryWrap><lido:repositorySet><lido:repositoryName><lido:legalBodyID lido:source="http://d-nb.info/gnd" lido:type="ID">16157973-5</lido:legalBodyID><lido:legalBodyName><lido:appellationValue lido:label="info:fedora/context:polos.repository.okc">Osrednja Knjižnica Celje</lido:appellationValue></lido:legalBodyName></lido:repositoryName><lido:workID lido:type="idno">Škatla 8; Laško 1, Rimske Toplice</lido:workID><lido:repositoryLocation><lido:namePlaceSet><lido:appellationValue>Muzejski trg 1a, SI - 3000 Celje</lido:appellationValue></lido:namePlaceSet></lido:repositoryLocation></lido:repositorySet></lido:repositoryWrap><lido:objectDescriptionWrap><lido:objectDescriptionSet><lido:descriptiveNoteValue><t:profileDesc><t:textClass><t:keywords xmlns:oth="http://gams.uni-graz.at/skos/scheme/o:oth/#" scheme="http://gams.uni-graz.at/skos/scheme/o:oth"><t:term/></t:keywords></t:textClass></t:profileDesc></lido:descriptiveNoteValue></lido:objectDescriptionSet><lido:objectDescriptionSet><lido:descriptiveNoteValue><t:profileDesc><t:textClass><t:keywords xmlns:ocm="http://gams.uni-graz.at/o:ocm/#" scheme="http://gams.uni-graz.at/skos/scheme/o:ocm"><t:term/></t:keywords></t:textClass></t:profileDesc></lido:descriptiveNoteValue></lido:objectDescriptionSet><lido:objectDescriptionSet lido:type="cirilo:normalizedPlaceNames"><lido:descriptiveNoteValue><t:profileDesc><t:textClass><t:keywords><t:list><t:item/></t:list></t:keywords></t:textClass></t:profileDesc></lido:descriptiveNoteValue></lido:objectDescriptionSet><lido:objectDescriptionSet lido:type="nationalFra